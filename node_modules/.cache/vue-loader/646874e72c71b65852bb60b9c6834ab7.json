\r\n        <!--主页组件HomeContainer.vue-->\r\n        <!--1.轮播图-->\r\n        <mt-swipe :auto=\"4000\">\r\n            <mt-swipe-item v-for=\"item in list\" :key=\"item.id\">   <!--加:key提高排序顺序-->\r\n                <img :src=\"item.img_url\"/>\r\n            </mt-swipe-item>\r\n        </mt-swipe>\r\n        <!--2.六宫格 start-->\r\n        <ul class=\"mui-table-view mui-grid-view mui-grid-9\">\r\n            <li class=\"mui-table-view-cell mui-media mui-col-xs-4 mui-col-sm-3\"><a href=\"#\">\r\n                    <img src=\"../../img/menu1.png\"/>\r\n                    <div class=\"mui-media-body\">新闻资讯</div></a></li>\r\n            <li class=\"mui-table-view-cell mui-media mui-col-xs-4 mui-col-sm-3\"><a href=\"#\">\r\n                    <img src=\"../../img/menu2.png\"/>\r\n                    <div class=\"mui-media-body\">商品</div></a></li>\r\n            <li class=\"mui-table-view-cell mui-media mui-col-xs-4 mui-col-sm-3\"><a href=\"#\">\r\n                    <img src=\"../../img/menu3.png\"/>\r\n                    <div class=\"mui-media-body\">点歌</div></a></li>\r\n            <li class=\"mui-table-view-cell mui-media mui-col-xs-4 mui-col-sm-3\"><a href=\"#\">\r\n                    <img src=\"../../img/menu4.png\"/>\r\n                    <div class=\"mui-media-body\">导航</div></a></li>\r\n            <li class=\"mui-table-view-cell mui-media mui-col-xs-4 mui-col-sm-3\"><a href=\"#\">\r\n                    <img src=\"../../img/menu5.png\"/>\r\n                    <div class=\"mui-media-body\">搜索</div></a></li>\r\n            <li class=\"mui-table-view-cell mui-media mui-col-xs-4 mui-col-sm-3\"><a href=\"#\">\r\n                    <img src=\"../../img/menu6.png\"/>\r\n                    <div class=\"mui-media-body\">购物车</div></a></li>\r\n        </ul> \r\n        <!--end-->\r\n    </div>\r\n</template>\r\n<script>\r\n    //加载当前组件需要的第三方模块\r\n    import {Toast} from \"mint-ui\";\r\n    export default{\r\n        data(){\r\n            return {\r\n                list:[]\r\n            }\r\n        },\r\n        methods:{\r\n            getImageList(){\r\n                //获取图片的轮播数据 \r\n                var url=\"http://127.0.0.1:3000/imagelist/list\"\r\n                this.$http.get(url).then(res=>{\r\n                    if(res.body.code==1){ \r\n                        //node.js发送的是json的字符串\r\n                        //通过vueresource接收自动将json字符串转换\r\n                        //js对象\r\n                        this.list=res.body.msg;\r\n                    }else{\r\n                        //出错提示 Toast() \r\n                        Toast(\"加载轮播图片失败...\");\r\n                    }\r\n                })\r\n            }\r\n        },\r\n        created(){\r\n            this.getImageList();\r\n        }\r\n    }\r\n\r\n</script>\r\n<style scoped>\r\n    /* 图片轮播的组件高度为0 */\r\n    .app .mint-swipe{\r\n        height:200px;\r\n    }\r\n    .app .mint-swipe img{\r\n        width:100%;\r\n        height:100%;\r\n    }\r\n    .app .mui-table-view.mui-grid-9{\r\n        background-color:#fff;\r\n    }\r\n    .app .mui-grid-view.mui-grid-9 .mui-table-view-cell{\r\n        border:0;\r\n    }\r\n    .app .mui-grid-view.mui-grid-9 .mui-table-view-cell img{\r\n        width:60px;\r\n        height:60px;\r\n    }\r\n</style>"]}]}